# Fibonacci sequence

func fibonacci(n: int) -> int {
    if n <= 0 {
        send 0;
    } else {
        if n == 1 {
            send 1;
        } else {
            send fibonacci(n - 1) + fibonacci(n - 2);
        }
    }
}

func main() {
    let i: int = 0;
    while i <= 10 {
        display fibonacci(i) , " ";
        i = i + 1;
    }
}